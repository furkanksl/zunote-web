{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {

  "*": ["./src/*", "../application/src/*"],
      "@/components/*": ["./components/*"],
      "@/services/*": ["./src/services/*"],
      "@/redux/*": ["./src/redux/*"],
      "@/models/*": ["./src/models/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "functions/index.js", "./*",],
  "exclude": ["node_modules"]
}
